<%= form_for (@tweet) do |f| %>
<div class="contents row">
  <div class="col s12 m6">
          <div class="card ">
            <div class="card-content black-text">
              <%= f.label :name %>
              <%= f.text_field :name, class: "card-title" %>
              <%= f.label :text %>
              <%= f.text_area :text, class: "materialize-textarea" %>

              </div>

            <div class="card-action">
               <%= f.submit "編集" %>
            </div>
          </div>
        </div>
      </div>
    <% end %>
